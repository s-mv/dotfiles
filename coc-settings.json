{
  "languageserver": {
  "ccls": {
    "command": "ccls",
    "filetypes": ["c", "cpp", "objc", "objcpp"],
    "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript"],
      "port": 6008
    }
  },
  "rpc": {
    "workspaceElapsedTime": true,
    "detailsEditing":"{workspace}",
    "checkIdle": false,
    "lowerDetailsEditing":"{filename}"
  }
}
